'Not(IsMatch)
Sub IsMatch(otherValue As String, tErrorMessage As String) As Boolean			'ignore
	Dim v As String = getValue
	v = UI.CStr(v)
	v = v.Trim
	otherValue = UI.CStr(otherValue)
	otherValue = otherValue.Trim
	If v = otherValue Then
		If sInputType = "legend" Then
			setBorderColor("success")
		Else
			setColor("success")
		End If
		Return False
	Else
		If sInputType = "legend" Then
			setBorderColor("error")
		Else
			setColor("error")
		End If
		Return True
	End If
End Sub
Sub IsMinLength(minLen As Int, tErrorMessage As String) As Boolean				'ignore
	Dim v As String = getValue
	v = UI.CStr(v)
	v = v.Trim
	If v.Length >= minLen Then
		If sInputType = "legend" Then
			setBorderColor("success")
		Else
			setColor("success")
		End If
		Return False
	Else
		If sInputType = "legend" Then
			setBorderColor("error")
		Else
			setColor("error")
		End If
		Return True
	End If
End Sub
'run validation
Sub IsBlank As Boolean
	Dim v As String = getValue
	v = UI.CStr(v)
	v = v.Trim
	If v = "" Then
		If sInputType = "legend" Then
			setBorderColor("error")
		Else
			setColor("error")
		End If
		HintError($"The ${sLabel.tolowercase} is required."$)
		Focus		
		Return True
	End If
	If sInputType = "legend" Then
		setBorderColor("success")
	Else
		setColor("success")
	End If
	setHint(sHint)
	Return False
End Sub

