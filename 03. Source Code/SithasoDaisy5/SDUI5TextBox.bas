                Return
            End If
			mTarget.Initialize($"#${sParentID}"$)
		End If
		Select Case sInputType
		Case "legend"	
			mElement = mTarget.Append($"[BANCLEAN]
				<fieldset id="${mName}_control" class="rounded-sm ${xclasses} fieldset" ${xattrs} style="${xstyles}">
