'load the items from a map
Sub SetOptionsFromMap(m As Map)		'ignoredeadcode
	If mElement = Null Then Return
	'clear existing items
	mElement.RunMethod("clear", Null)
	Dim nm As Map = CreateMap()
	If bValuesAsIs = False Then
		Dim mTot As Int = m.Size - 1
		Dim mCnt As Int = 0
		Dim nm As Map = CreateMap()
		For mCnt = 0 To mTot
			Dim k As String = m.GetKeyAt(mCnt)
			Dim v As String = m.GetValueAt(mCnt)
			k = UI.CleanID(k)
			nm.Put(k, v)
		Next
	Else
		nm = BANano.DeepClone(m)
	End If
	mElement.RunMethod("addOptionsMap", nm)
End Sub

'load the items from a list
Sub SetOptionsFromList(m As List)
	If mElement = Null Then Return
	Dim nm As Map = CreateMap()
	For Each k As String In m
		Dim sk As String = k
		If bValuesAsIs Then sk = UI.CleanID(sk)
		nm.Put(sk, k)
	Next
	SetOptionsFromMap(nm)
End Sub

