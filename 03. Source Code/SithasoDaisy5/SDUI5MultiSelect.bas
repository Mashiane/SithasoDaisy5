	If sChipTextColor <> "" Then UI.AddAttrDT("chip-text-color", sChipTextColor)
	If sDelimiter <> "" Then UI.AddAttrDT("delimiter", sDelimiter)
	If bHighlight = True Then UI.AddAttrDT("highlight", bHighlight)
	UI.AddAttrDT("max-selections", iMaxSelections)
	If bSearchable = True Then UI.AddAttrDT("searchable", bSearchable)
	If bShowSelectAll = True Then UI.AddAttrDT("show-select-all", bShowSelectAll)
	If bSingleSelect = True Then UI.AddAttrDT("single-select", bSingleSelect)
	If bVirtualScroll = True Then 
		UI.AddAttrDT("virtual-scroll", bVirtualScroll)
		UI.AddAttrDT("virtual-scroll-threshold", sVirtualScrollThreshold)
	End If	
	UI.AddAttrDT("chip-style", "true")
	Dim xattrs As String = UI.BuildExAttributes
	Dim xstyles As String = UI.BuildExStyle
	Dim xclasses As String = UI.BuildExClass
	If sParentID <> "" Then
