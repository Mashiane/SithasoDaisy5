'initialize the custom view class
Public Sub Initialize (Callback As Object, Name As String, EventName As String)
	If BANano.AssetsIsDefined("MultiSelect") = False Then
		BANano.Throw($"Uses Error: 'BANano.Await(app.UsesMultiSelect)' should be added for '${Name}'"$)
		Return
	End If
	UI.Initialize(Me)
	mElement = Null
	mEventName = UI.CleanID(EventName)
	mName = UI.CleanID(Name)
	mCallBack = Callback
	CustProps.Initialize
	BANano.DependsOnAsset("SVGRenderer.min.js")
	BANano.DependsOnAsset("daisy-multiselect.min.js")
	SetDefaults
	Options.Initialize 
End Sub

