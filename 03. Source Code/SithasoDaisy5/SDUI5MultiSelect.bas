'run validation
Sub IsBlank As Boolean
	Dim v As String = getValue
	v = UI.CStr(v)
	v = v.Trim
	If v = "" Then
		If sInputType = "legend" Then
			setBorderColor("error")
		Else
			setInputColor("error")
		End If
		HintError($"The ${sLabel.tolowercase} is required."$)
		Focus
		Return True
	End If
	If sInputType = "legend" Then
		setBorderColor("success")
	Else
		setInputColor("success")
	End If
	setHint(sHint)
	Return False
End Sub

Sub ResetValidation
	Try
		If sInputType = "legend" Then
			setBorderColor("success")
		Else
			setInputColor("success")
		End If
	Catch
		
	End Try		'ignore
End Sub

