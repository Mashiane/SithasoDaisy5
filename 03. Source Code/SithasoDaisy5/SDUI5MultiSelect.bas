		sLabel = Props.GetDefault("Label", "Select")
		sLabel = UI.CStr(sLabel)
		sPlaceholder = Props.GetDefault("Placeholder", "Select an element")
		sPlaceholder = UI.CStr(sPlaceholder)
		bRequired = Props.GetDefault("Required", False)
